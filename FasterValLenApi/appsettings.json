{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServerOptions": {
    "Port": 5000,
    "Address": "127.0.0.1",
    "MemorySize": "16g",
    "PageSize": "32m",
    "Segmentize": "1g",
    "IndexSize": "8g",
    "EnableStorageTier": false,
    "LogDir": null,
    "CheckpointDir": null,
    "Recover": false,
    "DisablePubSub": false,
    "PubSubPageSize": "4k"
  }
}